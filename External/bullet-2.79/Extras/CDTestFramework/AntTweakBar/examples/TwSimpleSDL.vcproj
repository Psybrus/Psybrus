			Name="Release|Win32"
			OutputDirectory="bin"
			IntermediateDirectory="bin"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="2"
